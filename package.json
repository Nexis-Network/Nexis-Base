    "alias": {
      "@": "./src",
      "@components": "./src/components",
      "@lib": "./src/lib"
    },
    "include": [
      "pages",
      "components",
      "lib",
      "utils",
      "hooks",
      "contexts",
      "styles",
      "config"
    ],
    "exclude": [],
    "entrypoints": [
      "pages",
      "pages/api",
      "components/layouts"
    ],
    "extensions": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx",
      ".mjs",
      ".cjs"
